#: forms.py:27 forms.py:107
msgid "Username"
msgstr "Identifiant"

#: forms.py:33 forms.py:79
msgid "Password"
msgstr "Mot de passe"

#: forms.py:37
msgid "Password (again)"
msgstr "Mot de passe (à nouveau)"

#: forms.py:41 forms.py:122 forms.py:168 forms.py:197
msgid "Email"
msgstr "Email"

#: forms.py:52
msgid "Usernames can only contain letters, numbers and underscores."
msgstr "L'identifiant ne peut contenir que des lettres, chiffres et tiret-bas (_)"

#: forms.py:60
msgid "This username is already taken. Please choose another."
msgstr "Cet identifiant est déjà utilisé. Merci d'en choisir un autre."

#: forms.py:67 forms.py:217
msgid "A user is registered with this email address."
msgstr "Un utilisateur est déjà enregistré avec cette adresse email."

#: forms.py:72 forms.py:162 forms.py:191
msgid "You must type the same password each time."
msgstr "Vous devez saisir le même mot de passe à chaque fois."

#: forms.py:83
msgid "Remember Me"
msgstr "Se souvenir de moi"

#: forms.py:96
msgid "This account is inactive."
msgstr "Ce compte est inactif."

#: forms.py:108
msgid "The username and/or password you specified are not correct."
msgstr "L'identifiant et/ou le mot de passe saisis ne sont pas corrects."

#: forms.py:123
msgid "The email address and/or password you specified are not correct."
msgstr "L'adresse email et/ou le mot de passe saisis ne sont pas corrects."

#: forms.py:138
msgid "Current Password"
msgstr "Mot de passe actuel"

#: forms.py:142 forms.py:180
msgid "New Password"
msgstr "Nouveau mot de passe"

#: forms.py:146 forms.py:184
msgid "New Password (again)"
msgstr "Nouveau mot de passe (à nouveau)"

#: forms.py:156
msgid "Please type your current password."
msgstr "Merci de saisir votre mot de passe actuel"

#: forms.py:173
msgid "Email address can not be found."
msgstr "L'adresse email n'a pas pu être trouvée."

#: forms.py:199
msgid "Timezone"
msgstr "Fuseau horaire"

#: forms.py:205
msgid "Language"
msgstr "Langue"

#: models.py:34
msgid "user"
msgstr "utilisateur"

#: models.py:35
msgid "timezone"
msgstr "fuseau horaire"

#: models.py:37
msgid "language"
msgstr "langue"

#: models.py:250
msgid "email address"
msgstr "adresse email"

#: models.py:251
msgid "email addresses"
msgstr "adresses email"

#: models.py:300
msgid "email confirmation"
msgstr "email de confirmation"

#: models.py:301
msgid "email confirmations"
msgstr "emails de confirmation"

#: views.py:42
#, python-brace-format
msgid "Confirmation email sent to {email}."
msgstr "L'email de confirmation a été envoyé à {email}."

#: views.py:46
#, python-brace-format
msgid "The code {code} is invalid."
msgstr "Le code {code} n'est pas valide."

#: views.py:379
#, python-brace-format
msgid "You have confirmed {email}."
msgstr "Vous avez validé l'adresse email {email}."

#: views.py:452 views.py:585
msgid "Password successfully changed."
msgstr "Le mot de passe a été modifié avec succès."

#: views.py:664
msgid "Account settings updated."
msgstr "Les paramètres de compte ont été mis à jour."

#: views.py:748
#, python-brace-format
msgid ""
"Your account is now inactive and your data will be expunged in the next "
"{expunge_hours} hours."
msgstr "Votre compte est maintenant inactif et vos données seront supprimées dans les {expunge_hours} prochaines heures."
