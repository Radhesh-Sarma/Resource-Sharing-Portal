#: forms.py:27 forms.py:107
msgid "Username"
msgstr "Nome de Usuário"

#: forms.py:33 forms.py:79
msgid "Password"
msgstr "Senha"

#: forms.py:37
msgid "Password (again)"
msgstr "Senha (novamente)"

#: forms.py:41 forms.py:122 forms.py:168 forms.py:197
msgid "Email"
msgstr "Email"

#: forms.py:52
msgid "Usernames can only contain letters, numbers and underscores."
msgstr "Nomes de usuário podem conter apenas letras, números e sublinhados."

#: forms.py:60
msgid "This username is already taken. Please choose another."
msgstr "Este nome de usuário já está cadastrado. Por favor, escolha outro."

#: forms.py:67 forms.py:217
msgid "A user is registered with this email address."
msgstr "Este endereço de email já foi registrado por outro usuário"

#: forms.py:72 forms.py:162 forms.py:191
msgid "You must type the same password each time."
msgstr "Você deve digitar a mesma senha."

#: forms.py:83
msgid "Remember Me"
msgstr "Lembrar-me"

#: forms.py:96
msgid "This account is inactive."
msgstr "Esta conta está inativa."

#: forms.py:108
msgid "The username and/or password you specified are not correct."
msgstr "O nome de usuário e/ou a senha informada não estão corretas"

#: forms.py:123
msgid "The email address and/or password you specified are not correct."
msgstr "O email e/ou a senha informada não estão corretas"

#: forms.py:138
msgid "Current Password"
msgstr "Senha atual"

#: forms.py:142 forms.py:180
msgid "New Password"
msgstr "Nova senha"

#: forms.py:146 forms.py:184
msgid "New Password (again)"
msgstr "Nova senha(novamente)"

#: forms.py:156
msgid "Please type your current password."
msgstr "Por favor, entre com sua senha atual"

#: forms.py:173
msgid "Email address can not be found."
msgstr "Endereço de email não pode ser encontrado."

#: forms.py:199
msgid "Timezone"
msgstr "Fuso Horário"

#: forms.py:205
msgid "Language"
msgstr "Idioma"

#: models.py:34
msgid "user"
msgstr "usuário"

#: models.py:35
msgid "timezone"
msgstr "fuso horário"

#: models.py:37
msgid "language"
msgstr "idioma"

#: models.py:250
msgid "email address"
msgstr "endereço de email"

#: models.py:251
msgid "email addresses"
msgstr "endereços de email"

#: models.py:300
msgid "email confirmation"
msgstr "email de confirmação"

#: models.py:301
msgid "email confirmations"
msgstr "confirmações por e-mail"

#: views.py:42
#, python-brace-format
msgid "Confirmation email sent to {email}."
msgstr "E-mail de confirmação enviado para {email}."

#: views.py:46
#, python-brace-format
msgid "The code {code} is invalid."
msgstr "O código {code} é inválido."

#: views.py:379
#, python-brace-format
msgid "You have confirmed {email}."
msgstr "Você confirmou {email}."

#: views.py:452 views.py:585
msgid "Password successfully changed."
msgstr "Senha alterada com sucesso."

#: views.py:664
msgid "Account settings updated."
msgstr "Configurações da conta atualizada."

#: views.py:748
#, python-brace-format
msgid ""
"Your account is now inactive and your data will be expunged in the next "
"{expunge_hours} hours."
msgstr "Sua conta agora está inativo e seus dados serão apagados nas próximas {expunge_hours} horas."
